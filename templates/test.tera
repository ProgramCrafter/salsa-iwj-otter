<body id="main-body" data-clientid="{{clientid}}">
<h1>Hi!</h1>
<div id="error"></div>
<p>
<div id="status">nothing</div>

<p>
    <svg id="space"
	 xmlns="http://www.w3.org/2000/svg" viewBox="0 0 400 200"
 	 onmousedown="drag_mousedown(event)"
	 >
      <rect fill="green" x="20" y="20" width="200" height="100"/>
{% for piece in uses %}{{piece}}{% endfor %}
      <use href="#piece42" data-p="42" x="50" y="80" />
      <use href="#piece43" data-p="43" x="50" y="60" />
      <use href="#select43" x="50" y="60" />
      <defs>
{% for piece in defs %}{{piece}}{% endfor %}
	<g id="piece42">
	  <circle
	    cx="0" cy="0"
	    fill="red" r="10"
	    />
	</g>
	<g id="base43">
	  <rect
	    x="-10" y="-10" width="20" height="20"
	    />
	</g>
	<g id="piece43">
	  <g fill="blue">
	    <use href="#base43">
	  </g>
	</g>
	<g id="select43">
	  <g stroke="black"
	     stroke-dasharray="3 1  1 1  1 1"
	     fill="none"
	   >
	  <g transform="scale(1.1)">
	    <use href="#base43">
	  </g>
	</g>
      </defs>
    </svg>

</body>
